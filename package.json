{
  "name": "is-file-open",
  "version": "0.1.1",
  "description": "Checks if a file is used by another process on macOS",
  "repository": "git@github.com:adambrgmn/is-file-open.git",
  "author": "Adam Bergman <adam@fransvilhelm.com>",
  "main": "lib/index.js",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/adambrgmn/is-file-open/issues"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "prebuild": "rimraf lib/*",
    "build": "babel src --out-dir lib",
    "test": "ava",
    "precommit": "lint-staged"
  },
  "devDependencies": {
    "ava": "^0.22.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.26.0",
    "eslint": "^4.7.1",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-config-prettier": "^2.5.0",
    "eslint-plugin-import": "^2.7.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.2.1",
    "prettier": "^1.7.0",
    "rimraf": "^2.6.2"
  },
  "lint-staged": {
    "*.js": [
      "prettier --single-quote --trailing-comma es5 --write",
      "git add"
    ]
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  }
}
